<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>admin</from-outcome>
			<to-view-id>/admin/WelcomeAdmin.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>büro</from-outcome>
			<to-view-id>/büro/WelcomeBüro.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>lager</from-outcome>
			<to-view-id>/lager/LagerView.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>lager/LagerView.xhtml</display-name>
		<from-view-id>/lager/LagerView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>rekla</from-outcome>
			<to-view-id>/claimInput.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>lager/LagerView.xhtml</display-name>
		<from-view-id>/lager/LagerView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>zservice</from-outcome>
			<to-view-id>/serviceInput.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>serviceInput.xhtml</display-name>
		<from-view-id>/serviceInput.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>lager</from-outcome>
			<to-view-id>/lager/LagerView.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>claimInput.xhtml</display-name>
		<from-view-id>/claimInput.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>lager</from-outcome>
			<to-view-id>/lager/LagerView.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
</faces-config>
